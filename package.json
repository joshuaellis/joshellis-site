    "clsx": "^1.1.1",
    "compression": "^1.7.4",
    "emit-file-loader": "0.0.2",
    "express": "^4.17.1",
    "immer": "^7.0.5",
    "next": "^9.4.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-lazy-load-image-component": "^1.3.2",
    "react-plx": "^1.3.14",
    "react-redux": "^7.2.0",
    "react-svg-loader": "^3.0.3",
    "react-three-fiber": "^4.2.12",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.5",
    "styled-components": "^5.1.1",
    "three": "^0.118.3"
