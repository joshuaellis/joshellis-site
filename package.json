{
  "name": "joshellis-site",
  "version": "3.0.1",
  "description": "my own personal site",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "lint": "TIMING=1 eslint --cache --cache-location=node_modules/.cache/.eslintcache . --ext .ts,.tsx",
    "lint:fix": "TIMING=1 eslint --cache --cache-location=node_modules/.cache/.eslintcache . --ext .ts,.tsx --fix",
    "prepare": "husky install",
    "start": "next start",
    "test": "yarn test:ts",
    "test:ts": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshuaellis/joshellis-site.git"
  },
  "author": "Josh Ellis",
  "license": "ISC",
  "homepage": "https://github.com/joshuaellis/joshellis-site#readme",
  "devDependencies": {
    "@commitlint/config-conventional": "^17.7.0",
    "@types/node": "^18.17.12",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "commitlint": "^17.7.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@next/mdx": "^13.4.19",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@react-spring/web": "^9.7.3",
    "@stitches/react": "^1.2.8",
    "@use-gesture/react": "^10.2.27",
    "@vercel/analytics": "^1.0.2",
    "hast-util-to-html": "7.1.3",
    "hast-util-to-string": "1.0.4",
    "next": "^13.4.19",
    "next-seo": "^6.1.0",
    "parse-numeric-range": "^1.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-wrap-balancer": "^1.1.0",
    "refractor": "3.6.0",
    "rehype-parse": "7.0.1",
    "remark-directive": "1.0.1",
    "unified": "9.2.2",
    "unist-util-visit": "2.0.3"
  },
  "lint-staged": {
    "*.{md,yaml,yml}": [
      "yarn prettier:check"
    ],
    "*.{js,ts,tsx}": [
      "yarn lint"
    ]
  },
  "packageManager": "yarn@3.6.3"
}
