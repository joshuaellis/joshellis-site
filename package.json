{
  "name": "joshellis-site",
  "version": "2.2.0",
  "description": "my own personal site",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV_CUSTOM=dev node ./server/server.js",
    "build": "NODE_ENV_CUSTOM=production next build",
    "start": "NODE_ENV_CUSTOM=production node ./server/server.js",
    "storybook": "start-storybook -p 9001 -s ./",
    "fetch:strings": "node ./tools/fetchStrings.js",
    "deploy:staging": "git push heroku-staging develop:master",
    "deploy:prod": "git push heroku-production master:master",
    "generate": "plop --plopfile tools/generators/index.js",
    "test": "standard",
    "format": "prettier-standard --format",
    "lint": "standard --fix && yarn format"
  },
  "repository": {
    "type": "git",
    "url": "git+https://joshua_ellis@bitbucket.org/joshua_ellis/joshellis.co.uk.git"
  },
  "engines": {
    "node": ">=14.0.0",
    "yarn": "^1.21.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      ".next/",
      "public/"
    ]
  },
  "author": "Josh Ellis",
  "license": "ISC",
  "homepage": "https://bitbucket.org/joshua_ellis/joshellis.co.uk#readme",
